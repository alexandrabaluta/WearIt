<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elevenzon.WearIt">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconita"
        android:label="WearIt"
        android:roundIcon="@drawable/iconita"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".FilterByActivity"></activity>
        <activity android:name=".ImagePreviewActivity" />
        <activity android:name=".PhotoTagActivity" />
        <activity
            android:name=".MyClothes"
            android:label="@string/title_activity_my_clothes"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GenerateOutfitActivity" />
        <activity android:name=".InspirationActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>